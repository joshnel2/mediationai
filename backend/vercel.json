{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/privacy-policy.html",
      "dest": "public/privacy-policy.html",
      "headers": {
        "cache-control": "s-maxage=86400"
      }
    },
    {
      "src": "/terms-of-service.html",
      "dest": "public/terms-of-service.html",
      "headers": {
        "cache-control": "s-maxage=86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}